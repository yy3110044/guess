<%@ page language="java" contentType="text/html;charset=UTF-8" pageEncoding="UTF-8"%><!DOCTYPE html>
<html>
<head>
<title>钱包 - <%=com.yy.guess.util.Util.getConfigCom(application).getWebTitle()%></title>
<%@include file="/m/head-common.jsp"%>
<script src="m/js/usercenter.js"></script>
</head>
<body>
	<div id="app">
		<nav data-v-1db5fc32="" class="app-header">
			<ul data-v-1db5fc32="">
				<li data-v-1db5fc32="" onclick="window.location.href='m/usercenter/menu.jsp'" class="base-icon history-back"></li>
				<li data-v-1db5fc32="">钱包</li>
				<li data-v-1db5fc32=""><a data-v-1db5fc32="" href="m/usercenter/wallet/tradeFlow.jsp" class="base-icon wallet-record"></a></li>
			</ul>
		</nav>
		<div class="weui-tab">
			<div id="vux_view_box_body" class="weui-tab__panel vux-fix-safari-overflow-scrolling">
				<div data-v-1419de5c="" class="wallet-page router-view">
					<section data-v-1419de5c="" class="wallet-header">
						<div data-v-1419de5c="" class="header-text">钱包余额</div>
						<div data-v-1419de5c="" class="header-money"></div>
						<div data-v-1419de5c="" class="withdrawal-button"><div onclick="window.location.href='m/usercenter/wallet/withdrawal.jsp'" data-v-1419de5c="" class="button-content">提款</div></div>
					</section>
					<section data-v-1419de5c="" class="wallet-body">
						<div data-v-1419de5c="" class="table-title">选择存款方式</div>
						<div data-v-1419de5c="" class="grid-content">
							<div onclick="window.location.href='m/usercenter/wallet/payOrder.jsp?payType=ALIPAY_WAP&payName='+encodeURI('支付宝')" data-v-1419de5c="" class="grid-item">
								<div data-v-1419de5c="" class="grid-icon" style="background-image: url('data:image/svg+xml;base64,');"></div>
								<div data-v-1419de5c="" class="grid-text">支付宝</div>
							</div>
							<div onclick="window.location.href='m/usercenter/wallet/payOrder2.jsp?payType=ALIPAY_WAP&payName='+encodeURI('定额支付宝')" data-v-1419de5c="" class="grid-item">
								<div data-v-1419de5c="" class="grid-icon" style="background-image: url('data:image/svg+xml;base64,');"></div>
								<div data-v-1419de5c="" class="grid-text">定额支付宝</div>
							</div>
							
							<%
							String userAgent = request.getHeader("user-agent");
							if(userAgent == null || userAgent.indexOf("bet999") < 0) {
							%>
							<div onclick="window.location.href='m/usercenter/wallet/payOrder.jsp?payType=WEIXIN_WAP&payName='+encodeURI('微信')" data-v-1419de5c="" class="grid-item">
								<div data-v-1419de5c="" class="grid-icon" style="background-image: url('data:image/svg+xml;base64,');"></div>
								<div data-v-1419de5c="" class="grid-text">微信</div>
							</div>
							<%}%>
						</div>
					</section>
				</div>
			</div>
		</div>
	</div>
</body>
</html>